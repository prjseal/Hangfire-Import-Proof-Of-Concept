{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "server=(local)\\SQLEXPRESS;database=HangfireDemo;user id=hangfire;password='hangfire';TrustServerCertificate=true;",
    "hangfireDB": "server=(local)\\SQLEXPRESS;database=hangfiredb;user id=hangfire;password='hangfire';TrustServerCertificate=true;",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.SqlClient"
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "a625c84f-0aa5-4345-a1c3-3b15e7a338d3",
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      }
    }
  },
  "AllowRunningHangfireJobs": false
}
